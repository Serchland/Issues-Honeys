<fluent:Button x:Class="IssuesHoneys.Modules.Projects.Views.AppRibbonTabButton"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:prism="http://prismlibrary.com/"
               prism:ViewModelLocator.AutoWireViewModel="True"
               xmlns:fluent="urn:fluent-ribbon"
               xmlns:core="clr-namespace:IssuesHoneys.Core.NameDefinition;assembly=IssuesHoneys.Core"
               Header="{x:Static core:Captions.ProjectsButtonCaption}"
               LargeIcon="{StaticResource Projects_ico}"
               Command="{Binding SelectedCommand}"
               CommandParameter="{x:Static core:ModuleNameParams.Projects}">
</fluent:Button>
